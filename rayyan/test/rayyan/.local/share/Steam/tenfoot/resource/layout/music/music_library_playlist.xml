<root>
	<styles>
		<include src="file://{resources}/styles/steamstyles.css" />
		<include src="file://{resources}/styles/music/music.css" />
		<include src="file://{resources}/styles/music/music_library.css" />
	</styles>
	
	<MusicLibraryPlaylistPanel onmoveup="MusicLibraryActivateGridTabs( PlaylistButton );"
			defaultfocus="MusicPlaylistGrid" selectionposboundary="vertical" >
		
		<Panel hittest="false" id="FastScrollWrapper">
			<Label id="FastScrollLabel" />
			<Label id="TypeToJumpLabel" />
		</Panel>
		
		<Grid id="MusicPlaylistGrid" class="MusicPlaylistGrid"
			scrolldirection="horizontal" verticalcount="7" horizontalcount="4" scaleoffset="0.78528" selectionpos="0,3" tabindex="auto">
		</Grid>
		
	</MusicLibraryPlaylistPanel>
	
</root>
